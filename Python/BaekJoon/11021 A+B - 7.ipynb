    "#11021 A+B - 7\n",
    "\n",
    "N = int(input())\n",
    "res = []\n",
    "for i in range(N):\n",
    "    a, b = map(int,input().split())\n",
    "    res.append(a+b)\n",
    "    \n",
    "for i in range(len(res)):\n",
    "    print(f'Case #{i+1}: {res[i]}')\n"
   ]
