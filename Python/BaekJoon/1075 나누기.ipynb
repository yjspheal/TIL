   "outputs": [],
   "source": [
    "N = int(input())\n",
    "F = int(input())\n",
    "\n",
    "N = N // 100 * 100\n",
    "\n",
    "for n in range(N, N+100):\n",
    "    if n % F == 0:\n",
    "        print(str(n)[-2:])\n",
    "        break\n"
   ]
