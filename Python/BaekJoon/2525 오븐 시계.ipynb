   "source": [
    "#2525 오븐 시계\n",
    "hour, minu = map(int,input().split())\n",
    "time = int(input())\n",
    "\n",
    "add_h = time // 60\n",
    "add_m = time % 60\n",
    "\n",
    "if add_m + minu >= 60:\n",
    "    add_h += 1\n",
    "    add_m -= 60\n",
    "    \n",
    "fin_h = (hour + add_h) % 24\n",
    "fin_m = minu + add_m\n",
    "\n",
    "print(fin_h, fin_m)"
   ]
  }
