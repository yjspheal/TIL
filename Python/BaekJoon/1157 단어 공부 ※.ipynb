   "source": [
    "#1157 단어 공부 ※\n",
    "\n",
    "word = list(input().upper())\n",
    "times = []\n",
    "newword = list(set(word))\n",
    "\n",
    "for w in newword:\n",
    "    times.append(word.count(w))\n",
    "\n",
    "max_t = max(times)\n",
    "\n",
    "if times.count(max_t) > 1:\n",
    "    print(\"?\")\n",
    "else:\n",
    "    print(newword[times.index(max(times))])"
   ]
  }
