    "\n",
    "while True:\n",
    "    N = input()\n",
    "    if int(N) == 0:\n",
    "        break\n",
    "    else:\n",
    "        rev = list(N)\n",
    "        N = list(N)\n",
    "        N.reverse()\n",
    "        if rev == N:\n",
    "            print(\"yes\")\n",
    "        else:\n",
    "            print(\"no\")\n",
