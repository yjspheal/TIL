    "#10809 알파벳 찾기\n",
    "from string import ascii_lowercase\n",
    "letters = list(ascii_lowercase)\n",
    "S = input()\n",
    "\n",
    "for alp in letters:\n",
    "    if alp in S:\n",
    "        print(S.index(alp),end=\" \")\n",
    "    else:\n",
    "        print(-1,end=\" \")"
   ]
