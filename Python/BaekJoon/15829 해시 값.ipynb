    "\n",
    "N = int(input())\n",
    "strs = input()\n",
    "\n",
    "hashi = 0\n",
    "r = 1\n",
    "M = 1234567891\n",
    "\n",
    "for s in strs:\n",
    "    a = letters.index(s) + 1\n",
    "    hashi += a * r\n",
    "    r *= 31\n",
    "\n",
    "print(hashi % 1234567891)"
